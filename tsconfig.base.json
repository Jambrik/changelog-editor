{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "target": "es2015",
    "strictNullChecks": false,
    "typeRoots": ["node_modules/@types"],
    "types": ["node", "@angular/localize"],
    "lib": ["es2018", "dom"],
    "downlevelIteration": true,
    "paths": {
      "@changelog-editor/feature/change-list": [
        "libs/changelog-editor/feature-change-list/src/index.ts"
      ],
      "@changelog-editor/feature/change-log-item": [
        "libs/changelog-editor/feature-change-log-item/src/index.ts"
      ],
      "@changelog-editor/feature/compact": [
        "libs/changelog-editor/feature-compact/src/index.ts"
      ],
      "@changelog-editor/feature/config-editor": [
        "libs/changelog-editor/feature-config-editor/src/index.ts"
      ],
      "@changelog-editor/feature/full": [
        "libs/changelog-editor/feature-full/src/index.ts"
      ],
      "@changelog-editor/feature/left-navbar": [
        "libs/changelog-editor/feature-left-navbar/src/index.ts"
      ],
      "@changelog-editor/feature/new-version-creation": [
        "libs/changelog-editor/feature-new-version-creation/src/index.ts"
      ],
      "@changelog-editor/feature/program-list": [
        "libs/changelog-editor/feature-program-list/src/index.ts"
      ],
      "@changelog-editor/feature/tag": [
        "libs/changelog-editor/feature-tag/src/index.ts"
      ],
      "@changelog-editor/feature/tags": [
        "libs/changelog-editor/feature-tags/src/index.ts"
      ],
      "@changelog-editor/feature/top-navbar": [
        "libs/changelog-editor/feature-top-navbar/src/index.ts"
      ],
      "@changelog-editor/util": ["libs/changelog-editor/util-src/index.ts"],
      "@changelog-editor/util/constants": [
        "libs/changelog-editor/util-constants/src/index.ts"
      ],
      "@changelog-editor/util/helpers": [
        "libs/changelog-editor/util-helpers/src/index.ts"
      ],
      "@changelog-editor/util/models": [
        "libs/changelog-editor/util-models/src/index.ts"
      ],
      "@changelog-editor/util/pipes": [
        "libs/changelog-editor/util-pipes/src/index.ts"
      ],
      "@changelog-editor/data-access-core": [
        "libs/changelog-editor/data-access-core/src/index.ts"
      ],
      "@changelog-editor/util/types": [
        "libs/changelog-editor/util-types/src/index.ts"
      ],
      "@changelog-editor/components/*": [
        "apps/changelog-editor/src/app/components/*"
      ],
      "@changelog-editor/environment": [
        "apps/changelog-editor/src/environments/environment"
      ]
    },
    "rootDir": ".",
    "skipLibCheck": true
  },
  "angularCompilerOptions": {
    "strictTemplates": true,
    "strictInputTypes": false,
    "strictNullInputTypes": false,
    "strictAttributeTypes": true,
    "strictOutputEventTypes": false,
    "strictDomEventTypes": false,
    "strictDomLocalRefTypes": false,
    "strictSafeNavigationTypes": true,
    "strictContextGenerics": true
  }
}
