{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "target": "es2015",
    "strictNullChecks": false,
    "typeRoots": ["node_modules/@types"],
    "types": ["node", "@angular/localize"],
    "lib": ["es2018", "dom"],
    "downlevelIteration": true,
    "paths": {
      "@changelog-editor/feature/change-list": [
        "libs/features/change-list/src/index.ts"
      ],
      "@changelog-editor/feature/change-log-item": [
        "libs/features/change-log-item/src/index.ts"
      ],
      "@changelog-editor/feature/compact": [
        "libs/features/compact/src/index.ts"
      ],
      "@changelog-editor/feature/config-editor": [
        "libs/features/config-editor/src/index.ts"
      ],
      "@changelog-editor/feature/full": ["libs/features/full/src/index.ts"],
      "@changelog-editor/feature/left-navbar": [
        "libs/features/left-navbar/src/index.ts"
      ],
      "@changelog-editor/feature/new-version-creation": [
        "libs/features/new-version-creation/src/index.ts"
      ],
      "@changelog-editor/feature/program-list": [
        "libs/features/program-list/src/index.ts"
      ],
      "@changelog-editor/feature/tag": ["libs/features/tag/src/index.ts"],
      "@changelog-editor/feature/tags": ["libs/features/tags/src/index.ts"],
      "@changelog-editor/feature/top-navbar": [
        "libs/features/top-navbar/src/index.ts"
      ],
      "@changelog-editor/util": ["libs/util/src/index.ts"],
      "@changelog-editor/util/constants": ["libs/util/constants/src/index.ts"],
      "@changelog-editor/util/helpers": ["libs/util/helpers/src/index.ts"],
      "@changelog-editor/util/models": ["libs/util/models/src/index.ts"],
      "@changelog-editor/util/pipes": ["libs/util/pipes/src/index.ts"],
      "@changelog-editor/util/services": ["libs/util/services/src/index.ts"],
      "@changelog-editor/util/types": ["libs/util/types/src/index.ts"],
      "@changelog-editor/components/*": [
        "apps/changelog-editor/src/app/components/*"
      ]
    },
    "rootDir": ".",
    "skipLibCheck": true
  },
  "angularCompilerOptions": {
    "strictTemplates": true,
    "strictInputTypes": false,
    "strictNullInputTypes": false,
    "strictAttributeTypes": true,
    "strictOutputEventTypes": false,
    "strictDomEventTypes": false,
    "strictDomLocalRefTypes": false,
    "strictSafeNavigationTypes": true,
    "strictContextGenerics": true
  }
}
